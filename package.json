{
  "name": "todo-electron-desktopApp",
  "productName": "Todo Desktop App",
  "version": "1.0.0",
  "description": "A feature-rich todo application with dark mode and spell checking",
  "main": "main.js",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "Todo Desktop App",
        "icon": "./assets/icon.icns",
        "platforms": [
          "darwin",
          "win32"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "todo-electron-desktopApp",
            "authors": "Your Name",
            "description": "A feature-rich todo application with dark mode and spell checking"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.9.0",
    "@electron-forge/maker-deb": "^7.9.0",
    "@electron-forge/maker-dmg": "^7.9.0",
    "@electron-forge/maker-rpm": "^7.9.0",
    "@electron-forge/maker-squirrel": "^7.9.0",
    "@electron-forge/maker-zip": "^7.9.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.9.0",
    "@electron-forge/plugin-fuses": "^7.9.0",
    "@electron/fuses": "^1.8.0",
    "electron": "38.1.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1"
  }
}
